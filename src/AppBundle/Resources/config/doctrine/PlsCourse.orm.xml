<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\PlsCourse" table="pls_course">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="courseVersion" type="integer" column="course_version" nullable="false"/>
    <field name="courseCode" type="string" column="course_code" length="50" nullable="true"/>
    <field name="courseName" type="string" column="course_name" length="255" nullable="true"/>
    <field name="courseCatId" type="integer" column="course_cat_id" nullable="true"/>
    <field name="mandatory" type="integer" column="mandatory" nullable="true"/>
    <field name="startDate" type="datetime" column="start_date" nullable="true"/>
    <field name="endDate" type="datetime" column="end_date" nullable="true"/>
    <field name="intruduce" type="string" column="intruduce" length="500" nullable="true"/>
    <field name="description" type="text" column="description" length="65535" nullable="true"/>
    <field name="trainerId" type="integer" column="trainer_id" nullable="true"/>
    <field name="createdDate" type="datetime" column="created_date" nullable="true"/>
    <field name="createdBy" type="integer" column="created_by" nullable="true"/>
    <field name="status" type="integer" column="status" nullable="true"/>
    <field name="approve" type="boolean" column="approve" nullable="true"/>
    <field name="hotCourse" type="boolean" column="hot_course" nullable="true"/>
    <field name="traineeCount" type="integer" column="trainee_count" nullable="true"/>
    <field name="lectureCount" type="integer" column="lecture_count" nullable="true"/>
    <field name="companyCode" type="string" column="company_code" length="255" nullable="true"/>
    <field name="avatar" type="string" column="avatar" length="255" nullable="true"/>
    <field name="rankCode" type="string" column="rank_code" length="300" nullable="true"/>
    <field name="regionCode" type="string" column="region_code" length="300" nullable="false"/>
    <field name="public" type="integer" column="public" nullable="false"/>
  </entity>
</doctrine-mapping>
