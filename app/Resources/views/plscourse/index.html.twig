{% extends 'base.html.twig' %}
{% block stylesheets %}
    <link rel="stylesheet" type="text/css" href="http://www.peoplelinkvietnam.com/onlinetraining/assets/5358c4e6/css/bootstrap.min.css"/>
    <link rel="stylesheet" type="text/css" href="http://www.peoplelinkvietnam.com/onlinetraining/webRTC/exportexcel/datatables_qq/dataTables.bootstrap.min.css">
{% endblock %}
{% block body %}
    <h1>Plscourses list</h1>

    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>Courseversion</th>
                <th>Coursecode</th>
                <th>Coursename</th>
                <th>Coursecatid</th>
                <th>Mandatory</th>
                <th>Startdate</th>
                <th>Enddate</th>
                <th>Intruduce</th>
                <th>Description</th>
                <th>Trainerid</th>
                <th>Createddate</th>
                <th>Createdby</th>
                <th>Status</th>
                <th>Approve</th>
                <th>Hotcourse</th>
                <th>Traineecount</th>
                <th>Lecturecount</th>
                <th>Companycode</th>
                <th>Avatar</th>
                <th>Rankcode</th>
                <th>Regioncode</th>
                <th>Public</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for plsCourse in plsCourses %}
            <tr>
                <td><a href="{{ path('plscourse_show', { 'id': plsCourse.id }) }}">{{ plsCourse.courseVersion }}</a></td>
                <td>{{ plsCourse.courseCode }}</td>
                <td>{{ plsCourse.courseName }}</td>
                <td>{{ plsCourse.courseCatId }}</td>
                <td>{{ plsCourse.mandatory }}</td>
                <td>{% if plsCourse.startDate %}{{ plsCourse.startDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if plsCourse.endDate %}{{ plsCourse.endDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ plsCourse.intruduce }}</td>
                <td>{{ plsCourse.description }}</td>
                <td>{{ plsCourse.trainerId }}</td>
                <td>{% if plsCourse.createdDate %}{{ plsCourse.createdDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ plsCourse.createdBy }}</td>
                <td>{{ plsCourse.status }}</td>
                <td>{% if plsCourse.approve %}Yes{% else %}No{% endif %}</td>
                <td>{% if plsCourse.hotCourse %}Yes{% else %}No{% endif %}</td>
                <td>{{ plsCourse.traineeCount }}</td>
                <td>{{ plsCourse.lectureCount }}</td>
                <td>{{ plsCourse.companyCode }}</td>
                <td>{{ plsCourse.avatar }}</td>
                <td>{{ plsCourse.rankCode }}</td>
                <td>{{ plsCourse.regionCode }}</td>
                <td>{{ plsCourse.public }}</td>
                <td>{{ plsCourse.id }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('plscourse_show', { 'id': plsCourse.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('plscourse_edit', { 'id': plsCourse.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('plscourse_new') }}">Create a new plsCourse</a>
        </li>
    </ul>
{% endblock %}
{% block javascripts %}
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.2.1.min.js"></script>
    <script src="http://www.peoplelinkvietnam.com/onlinetraining/assets/5358c4e6/js/bootstrap.min.js"></script>
    <script src="http://www.peoplelinkvietnam.com/onlinetraining/webRTC/exportexcel/jquery.dataTables.js"></script>
    <script src="http://www.peoplelinkvietnam.com/onlinetraining/webRTC/exportexcel/dataTables.bootstrap.js"></script>
    <script src="http://www.peoplelinkvietnam.com/onlinetraining/webRTC/exportexcel/datatables_qq/dataTables.select.min.js"></script>
    <script src="http://www.peoplelinkvietnam.com/onlinetraining/webRTC/exportexcel/datatables_qq/dataTables.buttons.min.js"></script>
    <script src="http://www.peoplelinkvietnam.com/onlinetraining/webRTC/exportexcel/datatables_qq/jszip.min.js"></script>
    <script src="http://www.peoplelinkvietnam.com/onlinetraining/webRTC/exportexcel/datatables_qq/buttons.html5.min.js"></script>
    <script>
    $(document).ready(function() {
        $('#example').DataTable( {
            dom: 'Bfrtip',
            buttons: [
                'excel'
            ],
            "scrollX": true,

        } );
        $('.buttons-excel').addClass('btn btn-info');
    });
    </script>
{% endblock %}